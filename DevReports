#Continuous Integration Demo
import java.util.Arrays;

public class ArrayExample {
    public static void main(String[] args) {
        int[] numbers = {1, 2, 3, 4, 5};

        // Call the method to double each element
        int[] doubledNumbers = doubleArrayElements(numbers);

        // Print the result
        System.out.println(Arrays.toString(doubledNumbers)); // Output: [2, 4, 6, 8, 10]
    }

    // Method to double each element in the array
    public static int[] doubleArrayElements(int[] arr) {
        int[] result = new int[arr.length]; // Create a new array to store the result
        for (int i = 0; i < arr.length; i++) {
            result[i] = arr[i] * 2; // Double each element
        }
        return result;
    }
}
